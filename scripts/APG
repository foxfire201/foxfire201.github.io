local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "komaru hub",
   Icon = 0,
   LoadingTitle = "Rayfield Interface Suite",
   LoadingSubtitle = "by Sirius",
   Theme = "Serenity",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil,
      FileName = "komaru hub"
   },

   Discord = {
      Enabled = true,
      Invite = "zNzJVgFGae",
      RememberJoins = true
   },

   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

local Tab = Window:CreateTab("autofarmy", 4483362458)
local Section = Tab:CreateSection("autofarm")
Tab:CreateDivider()

local auto = false
Tab:CreateToggle({
   Name = "autofarm sky v1",
   CurrentValue = false,
   Flag = "autofarm sky v1",
   Callback = function(Value)
    auto = Value
   end
})

function __main()
    task.spawn(function()
        repeat
            if auto then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(5001, 13, 5007)
            end
            task.wait()
        until nil
    end)
end

__main()
game.Players.LocalPlayer.CharacterAdded:Connect(__main)
